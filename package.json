{
  "name": "base_utils",
  "version": "0.0.1",
  "description": "Base module for onebyte micro service",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Faheem Alam <faheem.alam66@gmail.com>",
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "tslint --fix \"src/**/*.ts\"",
    "build": "npm run lint && npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "tsc --declaration",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "nyc --nycrc-path .nyrc_unit mocha --compilers ts-node/register --require source-map-support/register --full-trace \"tests/**/*.unit.ts\"",
    "test:integration": "nyc --nycrc-path .nyrc_integration mocha --compilers ts-node/register --require source-map-support/register --full-trace \"tests/**/*.integration.ts\" --exit"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.45",
    "@types/sinon": "5.0.2",
    "@types/sinon-chai": "3.2.1",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.5",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.0",
    "cross-env": "^5.0.1",
    "eslint-plugin-typescript": "^0.3.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.1",
    "sinon": "5.0.7",
    "sinon-chai": "3.3.0",
    "source-map-support": "^0.4.15",
    "standard": "^10.0.2",
    "supertest": "^3.3.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.11.0",
    "tslint-config-globalid": "^1.0.5",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^3.5.1",
    "typescript-eslint-parser": "^11.0.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ],
    "globals": [
      "sandbox",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "dependencies": {
    "@types/ajv": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.36",
    "@types/http-aws-es": "^1.1.2",
    "@types/lodash": "^4.14.91",
    "@types/node": "^8.0.17",
    "@types/sequelize": "^4.0.80",
    "ajv": "^5.2.4",
    "aws-sdk": "^2.94.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "cls-hooked": "^4.2.2",
    "decamelize": "^3.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "http-aws-es": "1.1.3",
    "ioredis": "^3.1.1",
    "jaeger-client": "^3.11.0",
    "lodash": "^4.17.4",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "sequelize": ">=5.3.0",
    "serialize-error": "^2.1.0",
    "umzug": "^2.2.0",
    "uuid": "^3.1.0"
  }
}
